    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="9.0.5" />
  </ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\NetDream.Shared\NetDream.Shared.csproj" />
		<ProjectReference Include="..\..\Shared\Providers\Providers.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="Market\Forms\" />
	  <Folder Include="Market\Models\" />
	  <Folder Include="Market\Repositories\" />
	  <Folder Include="Merchant\Forms\" />
	  <Folder Include="Merchant\Models\" />
	  <Folder Include="Merchant\Repositories\" />
	</ItemGroup>

</Project>
